{
  "name": "dashboard",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "sync-version": "node scripts/sync-version.js",
    "version:patch": "node scripts/bump-version.js patch && pnpm sync-version",
    "v": "node scripts/bump-version.js minor && pnpm sync-version",
    "version:ersion:minormajor": "node scripts/bump-version.js major && pnpm sync-version",
    "dev": "pnpm sync-version && next dev",
    "build": "pnpm sync-version && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "clsx": "^2.1.1",
    "next": "15.5.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.0",
    "react-redux": "^9.1.2",
    "use-sound": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-grid-layout": "^1.3.5",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.6",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vitest": "^4.0.6"
  },
  "packageManager": "pnpm@10.11.0"
}
